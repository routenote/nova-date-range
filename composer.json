{
    "name": "kpolicar/nova-date-range",
    "description": "Custom date range picker field for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "daterange",
        "date",
        "range",
        "from",
        "to",
        "multiple",
        "custom",
        "field"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Kpolicar\\DateRange\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kpolicar\\DateRange\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
